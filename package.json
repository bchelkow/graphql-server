{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "Example of GraphQL server.",
  "scripts": {
    "start": "nodemon ./server.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bchelkow/graphql-server.git"
  },
  "keywords": [
    "graphql",
    "example",
    "proxy"
  ],
  "author": "blazej chelkowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bchelkow/graphql-server/issues"
  },
  "homepage": "https://github.com/bchelkow/graphql-server#readme",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "babel-plugin-inline-import"
    ]
  },
  "dependencies": {
    "body-parser": "1.16.0",
    "cors": "2.8.1",
    "express": "4.14.0",
    "graphql-server-express": "0.5.1",
    "graphql-tools": "0.9.0",
    "lodash": "4.17.4",
    "request": "2.79.0",
    "request-promise": "4.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-plugin-inline-import": "2.0.4",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "graphql": "0.8.2",
    "nodemon": "1.11.0"
  }
}
